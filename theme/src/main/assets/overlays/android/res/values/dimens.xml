<!-- Based on cm-11 branch 14/05/16 -->
<resources>
    <!-- The width that is used when creating thumbnails of applications. -->
    <dimen name="thumbnail_width">164dp</dimen>
    <!-- The height that is used when creating thumbnails of applications. -->
    <dimen name="thumbnail_height">145dp</dimen>
    <!-- Height of the status bar -->
    <dimen name="status_bar_height">24dip</dimen>
    <!-- Width of the navigation bar when it is placed vertically on the screen -->
    <dimen name="navigation_bar_width">42dp</dimen>
    <!-- Size of notification text (see TextAppearance.StatusBar.EventContent) -->
    <dimen name="notification_text_size">14dp</dimen>
    <!-- Size of notification text titles (see TextAppearance.StatusBar.EventContent.Title) -->
    <dimen name="notification_title_text_size">18dp</dimen>
    <!-- Size of smaller notification text (see TextAppearance.StatusBar.EventContent.Line2, Info, Time) -->
    <dimen name="notification_subtext_size">12dp</dimen>
    <!-- Default padding for dialogs. -->
    <dimen name="dialog_padding">0dp</dimen>

    <!-- <dimen name="list_item_padding_horizontal_material">8dip</dimen> -->

    <!-- dimens_material -->

    <!-- Preference activity side margins -->
    <dimen name="preference_screen_side_margin_material">@*android:dimen/preference_screen_side_margin</dimen>
    <!-- Preference activity side margins negative-->
    <dimen name="preference_screen_side_margin_negative_material">@dimen/preference_screen_side_margin_material</dimen>
    <!-- Preference fragment padding, sides -->
    <dimen name="preference_fragment_padding_side_material">@dimen/preference_screen_side_margin_material</dimen>
    <dimen name="preference_screen_header_padding_side_material">@*android:dimen/preference_screen_header_padding_side</dimen>

    <!-- Default height of an action bar. -->
    <dimen name="action_bar_default_height_material">@*android:dimen/action_bar_default_height</dimen>
    <!-- Vertical padding around action bar icons. -->
    <dimen name="action_bar_icon_vertical_padding_material">@*android:dimen/action_bar_icon_vertical_padding</dimen>
    <!-- Top margin for action bar subtitles -->
    <dimen name="action_bar_subtitle_top_margin_material">@*android:dimen/action_bar_subtitle_top_margin</dimen>
    <!-- Bottom margin for action bar subtitles -->
    <dimen name="action_bar_subtitle_bottom_margin_material">@*android:dimen/action_bar_subtitle_bottom_margin</dimen>

    <dimen name="action_button_min_width_material">@*android:dimen/action_button_min_width</dimen>
    <dimen name="action_button_min_height_material">@*android:dimen/action_button_min_width</dimen>

    <dimen name="dialog_padding_material">@*android:dimen/dialog_padding</dimen>
    <dimen name="dialog_padding_top_material">@*android:dimen/dialog_padding</dimen>

    <!-- Dialog padding minus control padding, used to fix alignment. -->
    <dimen name="select_dialog_padding_start_material">20dp</dimen>
</resources>